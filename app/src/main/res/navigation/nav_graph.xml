<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/nav_graph"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.final_exam.fragments.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_navigation_home_to_navigation_play"
            app:destination="@id/navigation_play" />
    </fragment>
    <fragment
        android:id="@+id/navigation_play"
        android:name="com.example.final_exam.fragments.PlayFragment"
        android:label="PlayFragment" >
        <action
            android:id="@+id/action_navigation_play_to_navigation_home"
            app:destination="@id/navigation_home" />
    </fragment>
</navigation>